@model List<CodingEventsDemo.Models.Tag>

<h1>Event Tags</h1>

<p>
    <a asp-controller="Tag" asp-action="Create">Create Tags</a>
</p>

<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
    </tr>
    @foreach (var evttag in Model)
    {
        <tr>
            <td>@evttag.Id</td>
            <td><a asp-controller="Tag" asp-action="Detail" asp-route-id="@evttag.Id">@evttag.Name</a></td>
        </tr>
    }
</table>
